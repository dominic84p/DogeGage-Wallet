{
  "name": "desktop",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win"
  },
  "build": {
    "appId": "com.dogegage.wallet",
    "productName": "DogeGage Wallet",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "../index.html",
      "../app.js",
      "../worker.js",
      "../src/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.finance",
      "target": "dmg",
      "icon": "../src/assets/crypto/SVG/dgage.png",
      "identity": null
    },
    "win": {
      "target": "nsis",
      "icon": "../src/assets/crypto/SVG/dgage.png",
      "sign": null
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0"
  }
}